{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureDataFactorySwati"
		},
		"AzureBlobStorage_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_LS'"
		},
		"AzureSqlDatabase_Adventureworks_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_Adventureworks_LS'"
		},
		"AzureSqlDatabase_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase_LS'"
		},
		"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-swati.database.windows.net"
		},
		"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adventureworks"
		},
		"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sg0226093"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-swati.database.windows.net"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure-sql-swati"
		},
		"AzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sg0226093"
		},
		"RestService_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureDataFactorySwati_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_LS_connectionString": {
						"value": "[parameters('AzureBlobStorage_LS_connectionString')]"
					},
					"AzureSqlDatabase_Adventureworks_LS_password": {
						"value": "[parameters('AzureSqlDatabase_Adventureworks_LS_password')]"
					},
					"AzureSqlDatabase_LS_password": {
						"value": "[parameters('AzureSqlDatabase_LS_password')]"
					},
					"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_Adventureworks_LS_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_LS_properties_typeProperties_userName')]"
					},
					"RestService_LS_properties_typeProperties_url": {
						"value": "[parameters('RestService_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}